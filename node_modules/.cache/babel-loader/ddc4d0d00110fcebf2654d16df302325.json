{"ast":null,"code":"import { connectToBeacon, getMyAddress } from '../services/beacon-services.js';\nexport const connect = async () => {\n  const setAddress = \"dog\";\n  await connectToBeacon();\n  getMyAddress().then(setAddress);\n};\nexport default {\n  name: 'walletConnect',\n  props: {\n    msg: String,\n    setAddress: String\n  },\n  methods: {\n    connect\n  }\n};","map":{"version":3,"mappings":"AAgBA,SACEA,eADF,EAEEC,YAFF,QAGO,gCAHP;AAMA,OAAO,MAAMC,OAAM,GAAI,YAAY;EACjC,MAAMC,UAAS,GAAI,KAAnB;EACA,MAAMH,eAAe,EAArB;EACAC,YAAY,GAAGG,IAAf,CAAoBD,UAApB;AACF,CAJO;AAOP,eAAe;EACbE,IAAI,EAAE,eADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC,MADA;IAELL,UAAU,EAAEK;EAFP,CAFM;EAMbC,OAAO,EAAE;IACPP;EADO;AANI,CAAf","names":["connectToBeacon","getMyAddress","connect","setAddress","then","name","props","msg","String","methods"],"sourceRoot":"","sources":["/Users/benjaminwestbrook/Repositories/ninter/src/components/walletConnect.vue"],"sourcesContent":["\n\n<template>\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <h3>Essential Links</h3>\n    <ul>\n      <li><a href=\"https://vuejs.org\" target=\"_blank\" rel=\"noopener\">Connect Wallet</a></li>\n      <button @click.enter=this.connect>Mint it</button>\n    </ul>\n\n  </div>\n</template>\n\n<script>\n\nimport {\n  connectToBeacon,\n  getMyAddress\n} from '../services/beacon-services.js'\n\n\nexport const connect = async () => {\n  const setAddress = \"dog\"\n  await connectToBeacon()\n  getMyAddress().then(setAddress)\n}\n\n\nexport default {\n  name: 'walletConnect',\n  props: {\n    msg: String,\n    setAddress: String\n  },\n  methods: {\n    connect\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}