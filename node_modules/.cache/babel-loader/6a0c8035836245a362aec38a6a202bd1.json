{"ast":null,"code":"import { NODE_URL } from '../constants.js';\nimport { TezosToolkit } from '@taquito/taquito';\nimport { BeaconWallet } from '@taquito/beacon-wallet';\nlet globalWallet = undefined;\nconst options = {\n  name: 'MyAwesomeDapp',\n  iconUrl: 'https://tezostaquito.io/img/favicon.png',\n  preferredNetwork: \"ithacanet\",\n  eventHandlers: {\n    PERMISSION_REQUEST_SUCCESS: {\n      handler: async data => {\n        console.log('permission data:', data);\n      }\n    }\n  }\n};\nexport default {\n  props: [\"header_string\", \"information\"],\n  methods: {\n    connectWallet() {\n      const Tezos = new TezosToolkit(NODE_URL);\n\n      if (!globalWallet) {\n        // Create a new BeaconWallet instance. The options will be passed to the DAppClient constructor.\n        const wallet = new BeaconWallet(options); // Setting the wallet as the wallet provider for Taquito.\n\n        Tezos.setWalletProvider(wallet);\n        globalWallet = wallet;\n      }\n\n      console.log('globalWallet just after creating instance');\n      console.log(globalWallet);\n      return globalWallet;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,SAASA,QAAT,QAAyB,iBAAzB;AAEA,SAASC,YAAT,QAA6B,kBAA7B;AACA,SAASC,YAAT,QAA6B,wBAA7B;AAEA,IAAIC,YAAW,GAAIC,SAAnB;AAEA,MAAMC,OAAM,GAAI;EACdC,IAAI,EAAE,eADQ;EAEdC,OAAO,EAAE,yCAFK;EAGdC,gBAAgB,EAAE,WAHJ;EAIdC,aAAa,EAAE;IACbC,0BAA0B,EAAE;MAC1BC,OAAO,EAAE,MAAOC,IAAP,IAAgB;QACvBC,OAAO,CAACC,GAAR,CAAY,kBAAZ,EAAgCF,IAAhC;MACD;IAHyB;EADf;AAJD,CAAhB;AAeA,eAAe;EACXG,KAAK,EAAE,CACH,eADG,EAEH,aAFG,CADI;EAKXC,OAAO,EAAE;IACTC,aAAa,GAAG;MACd,MAAMC,KAAI,GAAI,IAAIjB,YAAJ,CAAiBD,QAAjB,CAAd;;MACE,IAAI,CAACG,YAAL,EAAmB;QACf;QACA,MAAMgB,MAAK,GAAI,IAAIjB,YAAJ,CAAiBG,OAAjB,CAAf,CAFe,CAGf;;QACAa,KAAK,CAACE,iBAAN,CAAwBD,MAAxB;QACAhB,YAAW,GAAIgB,MAAf;MACJ;;MACAN,OAAO,CAACC,GAAR,CAAY,2CAAZ;MACAD,OAAO,CAACC,GAAR,CAAYX,YAAZ;MACA,OAAOA,YAAP;IACJ;;EAbS;AALE,CAAf","names":["NODE_URL","TezosToolkit","BeaconWallet","globalWallet","undefined","options","name","iconUrl","preferredNetwork","eventHandlers","PERMISSION_REQUEST_SUCCESS","handler","data","console","log","props","methods","connectWallet","Tezos","wallet","setWalletProvider"],"sourceRoot":"","sources":["/Users/benjaminwestbrook/Repositories/ninter/src/components/walletConnect.vue"],"sourcesContent":["<template>\n    <div ref=\"walletConnectBar\">\n        <button @click=\"connectWallet\" ref=\"connectWalletButton\">Connect Wallet</button>\n    </div>\n</template>\n\n<script>\n\nimport { NODE_URL } from '../constants.js'\n\nimport { TezosToolkit } from '@taquito/taquito';\nimport { BeaconWallet } from '@taquito/beacon-wallet';\n\nlet globalWallet = undefined\n\nconst options = {\n  name: 'MyAwesomeDapp',\n  iconUrl: 'https://tezostaquito.io/img/favicon.png',\n  preferredNetwork: \"ithacanet\",\n  eventHandlers: {\n    PERMISSION_REQUEST_SUCCESS: {\n      handler: async (data) => {\n        console.log('permission data:', data);\n      },\n    },\n  },\n};\n\n\n\nexport default {\n    props: [\n        \"header_string\",\n        \"information\"\n        ],\n    methods: {\n    connectWallet() {\n      const Tezos = new TezosToolkit(NODE_URL)\n        if (!globalWallet) {\n            // Create a new BeaconWallet instance. The options will be passed to the DAppClient constructor.\n            const wallet = new BeaconWallet(options)\n            // Setting the wallet as the wallet provider for Taquito.\n            Tezos.setWalletProvider(wallet)\n            globalWallet = wallet\n        }\n        console.log('globalWallet just after creating instance')\n        console.log(globalWallet)\n        return globalWallet\n    }\n    }\n}\n\n</script>\n\n<style scoped>\n.modal{\n    width: 400px;\n    padding: 20px;\n    margin: 100px auto;\n    border-radius: 10px;\n}\n.backdrop{\n    top: 0;\n    background: rgb(0.1, 0.1, 0.1);\n    width: 100%;\n    height: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module"}