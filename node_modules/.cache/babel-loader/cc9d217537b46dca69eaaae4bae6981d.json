{"ast":null,"code":"import { BeaconWallet } from '@taquito/beacon-wallet';\nimport { TezosToolkit } from '@taquito/taquito';\nimport { NODE_URL, NFT_CONTRACT, NULL_STR } from '../constants';\nconst Tezos = new TezosToolkit(NODE_URL);\nexport default {\n  props: [\"header_string\", \"information\"],\n  methods: {\n    connectWallet() {\n      if (!globalWallet) {\n        // Create a new BeaconWallet instance. The options will be passed to the DAppClient constructor.\n        const wallet = new BeaconWallet({\n          name: 'TzButton'\n        }); // Setting the wallet as the wallet provider for Taquito.\n\n        Tezos.setWalletProvider(wallet);\n        globalWallet = wallet;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,SAASA,YAAT,QAA6B,wBAA7B;AACA,SAASC,YAAT,QAA6B,kBAA7B;AACA,SAASC,QAAT,EAAmBC,YAAnB,EAAiCC,QAAjC,QAAiD,cAAjD;AAEA,MAAMC,KAAI,GAAI,IAAIJ,YAAJ,CAAiBC,QAAjB,CAAd;AAEA,eAAe;EACXI,KAAK,EAAE,CACH,eADG,EAEH,aAFG,CADI;EAKXC,OAAO,EAAE;IACTC,aAAa,GAAG;MACX,IAAI,CAACC,YAAL,EAAmB;QAChB;QACA,MAAMC,MAAK,GAAI,IAAIV,YAAJ,CAAiB;UAAEW,IAAI,EAAE;QAAR,CAAjB,CAAf,CAFgB,CAIhB;;QACAN,KAAK,CAACO,iBAAN,CAAwBF,MAAxB;QACAD,YAAW,GAAIC,MAAf;MACJ;IACD;;EAVM;AALE,CAAf","names":["BeaconWallet","TezosToolkit","NODE_URL","NFT_CONTRACT","NULL_STR","Tezos","props","methods","connectWallet","globalWallet","wallet","name","setWalletProvider"],"sourceRoot":"","sources":["/Users/benjaminwestbrook/Repositories/ninter/src/components/walletConnect.vue"],"sourcesContent":["<template>\n    <div ref=\"walletConnectBar\">\n        <button @click=\"connectWallet\" ref=\"connectWalletButton\">Connect Wallet</button>\n    </div>\n</template>\n\n<script>\n\nimport { BeaconWallet } from '@taquito/beacon-wallet'\nimport { TezosToolkit } from '@taquito/taquito'\nimport { NODE_URL, NFT_CONTRACT, NULL_STR } from '../constants'\n\nconst Tezos = new TezosToolkit(NODE_URL);\n\nexport default {\n    props: [\n        \"header_string\",\n        \"information\"\n        ],\n    methods: {\n    connectWallet() {\n         if (!globalWallet) {\n            // Create a new BeaconWallet instance. The options will be passed to the DAppClient constructor.\n            const wallet = new BeaconWallet({ name: 'TzButton' })\n\n            // Setting the wallet as the wallet provider for Taquito.\n            Tezos.setWalletProvider(wallet)\n            globalWallet = wallet\n        }\n       }\n    }\n}\n\n</script>\n\n<style scoped>\n.modal{\n    width: 400px;\n    padding: 20px;\n    margin: 100px auto;\n    border-radius: 10px;\n}\n.backdrop{\n    top: 0;\n    background: rgb(0.1, 0.1, 0.1);\n    width: 100%;\n    height: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module"}