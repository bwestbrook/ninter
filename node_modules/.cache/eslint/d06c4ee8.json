wait Tezos.wallet.at(\n      \"KT1PWx2mnDueood7fEmfbBDKx1D9BAnnXitn\" // TZBTC\n    );\n    // Call a method on a contract\n    const result = await contract.methods\n      .transfer(\n        \"tz1d75oB6T4zUMexzkr5WscGktZ1Nss1JrT7\",\n        \"tz1Mj7RzPmMAqDUNFBn5t5VbXmWW4cSUAdtT\",\n        1\n      )\n      .send();\n    this.taquitoOperationHash = result.opHash;\n  }\n}\n</script>\n\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n",[],{"ruleId":null,"fatal":true,"severity":2,"message":"34","line":70,"column":9},"Parsing error: Unexpected token (15:9)"]